{
	"version": 1,
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"width": 1,
			"row": {
				"3": 9,
				"4": 1
			},
			"col": {
				"3": 2,
				"4": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "text_widget",
					"settings": {
						"title": "Is the data averaged ?",
						"size": "big",
						"value": "datasources[\"Sensors\"][\"isAverage\"] ? \"yes\" : \"no\"",
						"animate": true
					}
				}
			]
		},
		{
			"width": 1,
			"row": {
				"3": 1,
				"4": 7
			},
			"col": {
				"3": 3,
				"4": 3
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "gauge",
					"settings": {
						"title": "RPM",
						"value": "(datasources[\"Sensors\"][\"rpm\"] * 1).toFixed(1)",
						"units": "rpm",
						"min_value": 0,
						"max_value": "5000"
					}
				}
			]
		},
		{
			"width": 1,
			"row": {
				"3": 1,
				"4": 7
			},
			"col": {
				"3": 2,
				"4": 2
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "gauge",
					"settings": {
						"title": "Speed",
						"value": "(datasources[\"Sensors\"][\"speed\"] * 1).toFixed(1)",
						"units": "mph",
						"min_value": 0,
						"max_value": 100
					}
				}
			]
		},
		{
			"width": 1,
			"row": {
				"3": 1,
				"4": 7
			},
			"col": {
				"3": 1,
				"4": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "gauge",
					"settings": {
						"title": "Acceleration",
						"value": "(datasources[\"Sensors\"][\"acceleration\"] * 1).toFixed(3)",
						"units": "m/s^2",
						"min_value": 0,
						"max_value": "5"
					}
				}
			]
		}
	],
	"datasources": [
		{
			"name": "Sensors",
			"type": "JSON",
			"settings": {
				"url": "http://localhost:10101",
				"use_thingproxy": false,
				"refresh": 1,
				"method": "GET"
			}
		}
	],
	"columns": 3
}